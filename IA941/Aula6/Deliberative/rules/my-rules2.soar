sp {detect*state*success
   (state <s> ^DESIRED <desired>
              ^CURRENT <current>
              ^PLAN <plan>
              ^POSITION <position>
              ^io.input-link <il>
              ^io.output-link <ol>)
   (<plan> ^START <start>)
   (<start> ^NEXT <begin>)
   #(<il> ^CREATURE < creature>)
   #(<creature> ^DELIVERYSPOT <deliverySpot>)
   #(<deliverySpot> ^X <deliverySpotX> ^Y <deliverySpotY>)
   #(<position> ^X <deliverySpotX> ^Y <deliverySpotY>)
   #(<creature> ^PARAMETERS.SCORE <score> > 0)
   (<current> ^Red <r>
          ^Green <g>
          ^Blue <b>
          ^Yellow <y>
          ^Magenta <m>
          ^White <w>)
   (<desired> ^Red <r>
          ^Green <g>
          ^Blue <b>
          ^Yellow <y>
          ^Magenta <m>
          ^White <w>
   )
   
-->
   (<ol> ^TESTE <begin>)
   (write (crlf) |LEAFLET COMPLETED| )  
   (halt)
}





