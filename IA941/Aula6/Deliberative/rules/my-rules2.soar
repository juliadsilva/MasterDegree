   (<entity> ^STATUS <memoryItemStatus> = 0)
   (<s> ^DESIRED <desired>)
   (<s> ^CURRENT <current>)
   (<desired> ^<jewelColor> <dcolor> > 0)
   (<current> ^<jewelColor> <ccolor> < <dcolor>)
